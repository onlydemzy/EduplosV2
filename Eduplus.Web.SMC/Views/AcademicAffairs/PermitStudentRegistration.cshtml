
@{
    ViewBag.Title = "Permit Student Registration";

    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-3">
    <select data-bind="value:sessionId,options:sessions,optionsValue:'SessionId',optionsText:'Title',optionsCaption:'Choose Session'" class="form-control"></select>
</div>


<div class="col-md-3">
    <select data-bind="value:semesterId,options:semesters,optionsValue:'SemesterId',optionsText:'Title',optionsCaption:'Choose Semester'" class="form-control"></select>
</div>
    <div class="col-md-3">
        <input data-bind="value:matNo" class="form-control" placeholder="Student Matric Number"/>
    </div>
    <div class="col-md-3">
        <button data-bind="click:allow, disable:kill" class="btn btn-info">Allow</button>
    </div>
</div>
 @Scripts.Render("~/Scripts/ViewModels/courseRegPermitVM.js")