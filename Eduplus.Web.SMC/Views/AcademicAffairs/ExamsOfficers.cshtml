
@{
    ViewBag.Title = "ExamsOfficers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-group">

    <div class="col-md-4">
        <select data-bind="value:progType, options:progTypes,optionsText:'Type',optionsValue:'Type', optionsCaption:'Select Programme Type'" class="form-control"></select>
    </div>
</div>
<div class="form-group">

    <div class="col-md-4">
        <select data-bind="value:prog, options:progs,optionsText:'Title',optionsValue:'ProgrammeCode', optionsCaption:'Select Programme'" class="form-control"></select>
    </div>
</div>
<div class="row col-md-4">
    <table class="table table-sm">
        <thead>
            <tr><th>
                    Name
                </th>
            <th>Level</th>
            <th>Session Apptd</th>
            <th>IsActive</th>
            </tr>
        </thead>

    </table>
</div>
<div class="form-group col-md-4">

</div>
<script src="~/Scripts/ViewModels/examsOfficerVM.js"></script>