

@{
    ViewBag.Title = "Upload Result from Template";
     
}


@using (Html.BeginForm("UploadResultFromTemplate", "Result", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal" }))
{
   

    <div class="row">
        <div class="col-md-7">
           
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)

             
            <div class="form-group">
                <img id="output" style="max-height:200px; max-width:200px;" />
                <input type="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" name="result" id="result" value="Choose File" /> 
            </div>

            <div class="form-group">
                <input type="submit" class="btn btn-block btn-success" value="Upload and Continue"/>
            </div>

            <div class="form-group">
                @Html.ActionLink("Cancel","Index","Home")
            </div>
        </div>
    </div>
}
 

