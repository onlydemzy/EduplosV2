
@{
    ViewBag.Title = "ViewApplicants";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row col-md-12">
     
    <div class="form-group">
        <div class="col-md-10">
            <select data-bind="value:session,options:sessions,optionsValue:'Title',optionsText:'Title',optionsCaption:'Select Session'" class="form-control"></select>

        </div>

    </div>
    
    <div class="form-group">
        <div class="col-md-10">
            <select data-bind="value:progType,options:progTypes,optionsValue:'Type',optionsText:'Type',optionsCaption:'Select ProgrammeType'" class="form-control"></select>

        </div>

    </div>
    <div class="form-group">
        <button class="btn btn-info" data-bind="click:fetch">Fetch</button>
    </div>

</div>

@Scripts.Render("~/Scripts/ViewModels/viewApplicantsVM.js")