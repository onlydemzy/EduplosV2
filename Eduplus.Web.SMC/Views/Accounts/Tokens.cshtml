
@{
    ViewBag.Title = "All Tokens";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h2>Tokens</h2>
    <hr />
    <p><a class="btn btn-success" data-bind="click:$root.add" href="#" title="edit"><i class="icon-plus"></i> Add Tokens</a></p>
    <div class="form-group"><input type="text" class="form-control" data-bind="value:$root.companyname, visible:show" placeholder="Enter Company name"/>
        
    </div>
    <div class="form-group">
        
        <input type="text" class="form-control" data-bind="value:$root.url, visible:show" placeholder="Company url" />
        
    </div>
    <div class="form-group">
        
        <a class="btn btn-success" data-bind="click:$root.save" href="#" title="edit"><i class="icon-plus"></i> Save</a>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Company</th>
                
                <th style="width: 120px; text-align:right;" />
            </tr>
        </thead>
        <tbody data-bind="foreach:tokens">
            <tr>
                <td data-bind="text:TokenId"></td>
                <td data-bind="text:Company"></td>


                <td class="buttons">
                    <a class="btn btn-success" data-bind="click: $root.view" href="#" title="View details"><i class="glyphicon glyphicon-edit"></i></a>

                </td>

            </tr>
        </tbody>

        
    </table>


   

</div>
    @Scripts.Render("/Scripts/ViewModels/tokensVM.js")




